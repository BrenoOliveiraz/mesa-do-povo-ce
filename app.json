{
  "expo": {
    "name": "mesa-do-povo-ce",
    "slug": "mesa-do-povo-ce",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "mesadopovoce",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.brenooouuu.mesadopovoce",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {},
      "edgeToEdgeEnabled": true,
      "package": "com.brenooouuu.mesadopovoce"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0a26f583-5bb9-4ff7-9aa1-69cdc26ddae0"
      }
    }
  }
}
